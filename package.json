{
	"name": "wyliodrinstudio",
	"version": "2.0.3-beta",
	"description": "Wyliodrin STUDIO 2",
	"main": "main.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "electron build/",
		"postinstall": "mkdirp build && webpack && electron-builder install-app-deps",
		"pack": "webpack --mode=production && electron-builder --dir",
		"dist": "webpack --mode=production && electron-builder"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alexandruradovici/WyliodrinSTUDIO2.git"
	},
	"author": "Wyliodrin SRL",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/alexandruradovici/WyliodrinSTUDIO2/issues"
	},
	"build": {
		"appId": "wyliodrin.studio",
		"mac": {
			"category": "public.app-category.developer-tools",
			"icon": "source/img/icons/mac/wyliodrin-studio-logo.icns"
		},
		"linux": {
			"desktop": {
				"Name": "Wyliodrin STUDIO",
				"Type": "Application",
				"Terminal": false
			},
			"category": "Development",
			"vendor": "Wyliodrin SRL",
			"icon": "source/img/wyliodrin-studio-logo.png"
		},
		"win": {
			"icon": "source/img/icons/win/wyliodrin-studio-logo.ico"
		},
		"copyright": "Copyright &copy; 2019, Wyliodrin SRL",
		"productName": "Wyliodrin STUDIO",
		"directories": {
			"app": "build"
		}
	},
	"homepage": "https://github.com/alexandruradovici/WyliodrinSTUDIO2#readme",
	"devDependencies": {
		"@mdi/font": "^4.2.95",
		"brace": "^0.11.1",
		"copy-webpack-plugin": "^5.0.3",
		"css-loader": "^3.0.0",
		"dexie": "^2.0.4",
		"electron": "^5.0.6",
		"electron-builder": "^20.44.4",
		"eslint": "^6.0.1",
		"eslint-plugin-vue": "^5.2.3",
		"file-loader": "^4.0.0",
		"highcharts": "^7.1.2",
		"highcharts-vue": "^1.3.5",
		"highlight.js": "^9.15.8",
		"html-webpack-plugin": "^3.2.0",
		"jquery": "^3.4.1",
		"jszip": "^3.2.2",
		"katex": "^0.10.2",
		"less": "^3.9.0",
		"less-loader": "^5.0.0",
		"marked": "git+https://github.com/wyliodrinstudio/marked.git",
		"material-design-icons-iconfont": "^4.0.5",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.26",
		"msgpack5": "^4.2.1",
		"node-blockly": "https://github.com/wyliodrinstudio/node-blockly.git",
		"raw-loader": "^3.1.0",
		"reconnectingwebsocket": "^1.0.0",
		"uuid": "^3.3.2",
		"validator": "^10.11.0",
		"vue": "^2.6.10",
		"vue-chartjs": "^3.4.2",
		"vue-clickaway": "^2.2.2",
		"vue-devtools": "^5.1.0",
		"vue-i18n": "^8.8.0",
		"vue-loader": "^15.7.0",
		"vue-resource": "^1.5.1",
		"vue-router": "^3.0.2",
		"vue-swatches": "^1.0.3",
		"vue-template-compiler": "^2.6.10",
		"vue2-ace-editor": "0.0.13",
		"vue2-highcharts": "^1.2.5",
		"vuedl": "^0.3.4",
		"vuetify": "^2.0.1",
		"vuetify-dialog": "^0.3.6",
		"vuex": "^3.1.0",
		"webpack": "^4.35.0",
		"webpack-cli": "^3.3.5",
		"webpack-node-externals": "^1.7.2",
		"xml-js": "^1.6.11",
		"xterm": "^3.14.5"
	},
	"eslintConfig": {
		"env": {
			"es6": true,
			"node": true,
			"browser": true,
			"mocha": false
		},
		"globals": {
			"node": true
		},
		"parserOptions": {
			"ecmaVersion": 2018,
			"sourceType": "module"
		},
		"plugins": [
			"vue"
		],
		"extends": [
			"eslint:recommended",
			"plugin:vue/essential"
		],
		"rules": {
			"indent": [
				"error",
				"tab",
				{
					"SwitchCase": 1
				}
			],
			"linebreak-style": [
				"error",
				"unix"
			],
			"no-console": [
				"warn"
			],
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			],
			"require-await": [
				"error"
			]
		}
	},
	"optionalDependencies": {
		"drivelist": "^8.0.6",
		"serialport": "^7.1.4"
	},
	"dependencies": {
		"@abandonware/noble": "^1.9.2-2",
		"aedes": "^0.39.0",
		"architect": "^0.1.13",
		"archiver": "^3.0.0",
		"bonjour-hap": "^3.5.4",
		"desktop-screenshot": "^0.1.1",
		"electron-is-dev": "^1.1.0",
		"fs-extra": "^7.0.1",
		"grabity": "^1.0.5",
		"irc-upd": "^0.10.0",
		"material-design-icons": "^3.0.1",
		"material-icons": "^0.3.1",
		"node-notifier": "^5.4.0",
		"screenshot-desktop": "^1.8.0",
		"ssh2": "^0.8.2",
		"vue-filereader": "^0.1.1"
	}
}
