<toolbox>
    <category name="Timing" colour="17">
        <block type="wyliodrin_delay">
            <value name="millis">
                <block type="math_number">
                    <field name="NUM">1000</field>
                </block>
            </value>
        </block>
    </category>
    <category name="Pin Access" colour="17">
        <block type="pinmode">
            <value name="pin">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
        </block>
        <block type="digitalwrite">
            <value name="pin">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
            <value name="value">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
        </block>
        <block type="digitalread">
            <value name="pin">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
        </block>
        <block type="analogwrite">
            <value name="pin">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
            <value name="value">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
        </block>
        <block type="analogread">
            <value name="pin">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
        </block>
    </category>
    <category name= "Peripherals">
        <category name="Pins" colour="17">
            <block type="setpin">
                <value name="pin">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
            </block>
            <block type="readpin">
                <value name="pin">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
            </block>
            <block type="shiftout">
                <value name="data_pin">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
                <value name="clock_pin">
                    <block type="math_number">
                        <field name="NUM">1</field>
                    </block>
                </value>
                <value name="value">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
            </block>
            <block type="shiftin">
                <value name="data_pin">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
                <value name="clock_pin">
                    <block type="math_number">
                        <field name="NUM">1</field>
                    </block>
                </value>
            </block>
        </category>
        <category name="LED" colour="260">
            <block type="set_led">
                <value name="pin">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
            </block>
            <block type="set_rgb_led">
                <value name="color">
                    <block type="colour_picker">
                        <field name="COLOUR">#ff0000</field>
                    </block>
                </value>
                <value name="red">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
                <value name="green">
                    <block type="math_number">
                        <field name="NUM">1</field>
                    </block>
                </value>
                <value name="blue">
                    <block type="math_number">
                        <field name="NUM">2</field>
                    </block>
                </value>
            </block>
            <block type="set_fine_rgb_led">
                <value name="color">
                    <block type="colour_picker">
                        <field name="COLOUR">#ff9966</field>
                    </block>
                </value>
                <value name="red">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
                <value name="green">
                    <block type="math_number">
                        <field name="NUM">1</field>
                    </block>
                </value>
                <value name="blue">
                    <block type="math_number">
                        <field name="NUM">2</field>
                    </block>
                </value>
            </block>
        </category>
        <category name="7 Segment Display" colour="180">
            <block type="variables_set" id="4" inline="true" x="70" y="47">
                <field name="VAR">seg7</field>
                <value name="VALUE">
                    <block type="sevensegmdispl_setup">
                        <value name="seg_a">
                            <block type="math_number">
                                <field name="NUM">0</field>
                            </block>
                        </value>
                        <value name="seg_b">
                            <block type="math_number">
                                <field name="NUM">1</field>
                            </block>
                        </value>
                        <value name="seg_c">
                            <block type="math_number">
                                <field name="NUM">2</field>
                            </block>
                        </value>
                        <value name="seg_d">
                            <block type="math_number">
                                <field name="NUM">3</field>
                            </block>
                        </value>
                        <value name="seg_e">
                            <block type="math_number">
                                <field name="NUM">4</field>
                            </block>
                        </value>
                        <value name="seg_f">
                            <block type="math_number">
                                <field name="NUM">5</field>
                            </block>
                        </value>
                        <value name="seg_g">
                            <block type="math_number">
                                <field name="NUM">6</field>
                            </block>
                        </value>
                        <value name="seg_dp">
                            <block type="math_number">
                                <field name="NUM">7</field>
                            </block>
                        </value>
                    </block>
                </value>
            </block>
            <block type="sevensegmdispl_display">
                <value name="ssd">
                    <block type="variables_get">
                        <field name="VAR">seg7</field>
                    </block>
                </value>
                <value name="tobedispl">
                    <block type="text">
                        <field name="TEXT">0</field>
                    </block>
                </value>
            </block>
        </category>
        <category name="4x7segment" colour="260">
            <block type="variables_set" id="4" inline="true" x="70" y="47">
                <field name="VAR">4x7segment</field>
                <value name="VALUE">
                    <block type="adafruit_7segment_begin">
                        <value name="bus">
                            <block type="math_number">
                                <field name="NUM">112</field>
                            </block>
                        </value>
                    </block>
                </value>
            </block>
            <block type="adafruit_7segment_print">
                <value name="nr">
                    <block type="math_number">
                        <field name="NUM">100</field>
                    </block>
                </value>
                <value name="7seg">
                    <block type="variables_get">
                        <field name="VAR">4x7segment</field>
                    </block>
                </value>
            </block>
            <block type="adafruit_writeDisplay">
                <value name="adafruit">
                    <block type="variables_get">
                        <field name="VAR">4x7segment</field>
                    </block>
                </value>
            </block>
        </category>
        <category name="24LedBar" colour="260">
            <block type="variables_set" id="4" inline="true" x="70" y="47">
                <field name="VAR">24LedBar</field>
                <value name="VALUE">
                    <block type="adafruit_24bar_begin">
                        <value name="bus">
                            <block type="math_number">
                                <field name="NUM">112</field>
                            </block>
                        </value>
                    </block>
                </value>
            </block>
            <block type="adafruit_24bar_color">
                <value name="led">
                    <block type="math_number">
                        <field name="NUM">1</field>
                    </block>
                </value>
                <value name="24bar">
                    <block type="variables_get">
                        <field name="VAR">24LedBar</field>
                    </block>
                </value>
            </block>
            <block type="adafruit_writeDisplay">
                <value name="adafruit">
                    <block type="variables_get">
                        <field name="VAR">24LedBar</field>
                    </block>
                </value>
            </block>
        </category>
        <category name="LCD" colour="260">
            <block type="init_lcd_i2c">
                <field name="rows">2</field>
                <value name="i2caddress">
                    <block type="math_number">
                        <field name="NUM">32</field>
                    </block>
                </value>
            </block>
            <block type="init_lcd">
                <field name="rows">2</field>
                <value name="rs">
                    <block type="math_number">
                        <field name="NUM">10</field>
                    </block>
                </value>
                <value name="strobe">
                    <block type="math_number">
                        <field name="NUM">11</field>
                    </block>
                </value>
                <value name="d0">
                    <block type="math_number">
                        <field name="NUM">5</field>
                    </block>
                </value>
                <value name="d1">
                    <block type="math_number">
                        <field name="NUM">6</field>
                    </block>
                </value>
                <value name="d2">
                    <block type="math_number">
                        <field name="NUM">7</field>
                    </block>
                </value>
                <value name="d3">
                    <block type="math_number">
                        <field name="NUM">9</field>
                    </block>
                </value>
            </block>
            <block type="clear_lcd"></block>
            <block type="reset_lcd"></block>
            <block type="set_position_lcd">
                <value name="col">
                    <block type="math_number">
                        <field name="NUM">1</field>
                    </block>
                </value>
                <value name="row">
                    <block type="math_number">
                        <field name="NUM">1</field>
                    </block>
                </value>
            </block>
            <block type="print_lcd">
                <value name="text">
                    <block type="text">
                        <field name="TEXT">Hey Wyliodrin</field>
                    </block>
                </value>
            </block>
        </category>
    </category>
    <category name="Sensors">
        <category name="Humidity sensor" colour="225">
            <block type="variables_set" id="4" inline="true" x="70" y="47">
                <field name="VAR">humiditySensor</field>
                <value name="VALUE">
                    <block type='htu21d_setup'></block>
                </value>
            </block>
            <block type='htu21d_getHum'>
                <value name="humidity">
                    <block type="variables_get">
                        <field name="VAR">humiditySensor</field>
                    </block>
                </value>
            </block>
            <block type='htu21d_getTem'>
                <value name="hum">
                    <block type="variables_get">
                        <field name="VAR">humiditySensor</field>
                    </block>
                </value>
            </block>
        </category>
        <category name="Accelerometer" colour="225">
            <block type="variables_set" id="4" inline="true" x="70" y="47">
                <field name="VAR">accelerometer</field>
                <value name="VALUE">
                    <block type="mpu6050_setup">
                        <value name="address">
                            <block type="math_number">
                                <field name="NUM">104</field>
                            </block>
                        </value>
                    </block>
                </value>
            </block>
            <block type='mpu6050_getAccX'>
                <value name="accelerom">
                    <block type="variables_get">
                        <field name="VAR">accelerometer</field>
                    </block>
                </value>
            </block>
            <block type='mpu6050_getAccY'>
                <value name="acceleromy">
                    <block type="variables_get">
                        <field name="VAR">accelerometer</field>
                    </block>
                </value>
            </block>
            <block type='mpu6050_getAccZ'>
                <value name="acceleromz">
                    <block type="variables_get">
                        <field name="VAR">accelerometer</field>
                    </block>
                </value>
            </block>
            <block type='mpu6050_getTemp'>
                <value name="acceleromt">
                    <block type="variables_get">
                        <field name="VAR">accelerometer</field>
                    </block>
                </value>
            </block>
            <block type='mpu6050_getRotX'>
                <value name="acceleromm">
                    <block type="variables_get">
                        <field name="VAR">accelerometer</field>
                    </block>
                </value>
            </block>
            <block type='mpu6050_getRotY'>
                <value name="accelerommy">
                    <block type="variables_get">
                        <field name="VAR">accelerometer</field>
                    </block>
                </value>
            </block>
            <block type='mpu6050_getRotZ'>
                <value name="accelerommz">
                    <block type="variables_get">
                        <field name="VAR">accelerometer</field>
                    </block>
                </value>
            </block>
        </category>
        <category name="Buttons" colour="17">
            <block type="button_is">
                <value name="pin">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
            </block>
            <block type="button_switched">
                <value name="pin">
                    <block type="math_number">
                        <field name="NUM">0</field>
                    </block>
                </value>
            </block>
        </category>
        <category name="Barometer" colour="120">
            <block type="bmp180_get_pressure"></block>
            <block type="bmp180_get_temperature"></block>
        </category>
    </category>
    <category name="Signals" colour="330">
        <block type="send_signals">
            <value name="value_signal">
                <block type="text">
                    <field name="TEXT"></field>
                </block>
            </value>
            <value name="value_value">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
        </block>
        <block type="signal_receivesignal">
            <value name="signal">
                <block type="text">
                    <field name="TEXT"></field>
                </block>
            </value>
            <value name="signalvalue">
                <block type="variables_get">
                    <field name="VAR">signalvalue</field>
                </block>
            </value>
        </block>
        <block type="sendsignals">
            <value name="value_signal">
                <block type="lists_create_with">
                    <mutation items="1"/>
                </block>
            </value>
            <value name="value_value">
                <block type="lists_create_with">
                    <mutation items="1"/>
                </block>
            </value>
        </block>
        <block type="sendsignalsandflag">
            <value name="value_signal">
                <block type="lists_create_with">
                    <mutation items="1"/>
                </block>
            </value>
            <value name="value_value">
                <block type="lists_create_with">
                    <mutation items="1"/>
                </block>
            </value>
            <value name="debug_text">
                <block type="text">
                    <field name="TEXT"></field>
                </block>
            </value>
        </block>
        <block type="signal_sendanddebug">
            <value name="signal_text">
                <block type="text">
                    <field name="TEXT"></field>
                </block>
            </value>
            <value name="signal_value">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
            <value name="debug_text">
                <block type="text">
                    <field name="TEXT"></field>
                </block>
            </value>
        </block>
        <block type="signal_putflag">
            <value name="signal_text">
                <block type="text">
                    <field name="TEXT"></field>
                </block>
            </value>
            <value name="flag_text">
                <block type="text">
                    <field name="TEXT"></field>
                </block>
            </value>
        </block>
        <block type="sendcoord">
            <value name="name">
                <block type="text">
                    <field name="TEXT">...</field>
                </block>
            </value>
            <value name="latitudine">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
            <value name="longitudine">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
        </block>
        <block type="sendcoordandflag">
            <value name="name">
                <block type="text">
                    <field name="TEXT">...</field>
                </block>
            </value>
            <value name="latitudine">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
            <value name="longitudine">
                <block type="math_number">
                    <field name="NUM">0</field>
                </block>
            </value>
            <value name="flag">
                <block type="text">
                    <field name="TEXT">...</field>
                </block>
            </value>
        </block>
    </category>
    <sep />
</toolbox>